#: ../src/keyboard_gui.py.orig:105
msgid "Keyboard"
msgstr "Ibhodi lezinkinobho"

#: ../src/keyboard_cli.py:35
msgid ""
"Usage: system-config-keyboard [--help] [--noui] [--text] [<keyboardtype>]\n"
"       --help            Print out this message.\n"
"       --noui            Run in command line mode.\n"
"       --text            Run in text interface mode.\n"
"       "
msgstr ""
"Ukusetshenziswa: ibhodi lezikhiye le-redhat [--usizo] [--noui] [--umbhalo]"
"[<keyboardtype>]\n"
"       --usizo            Shicilela lo myalezo.\n"
"       --noui            Sebenzisa endleleni yomugqa womyalelo.\n"
"       --umbhalo            Sebenzisa endleleni yoxhumano lombhalo.\n"
"       "

#: ../src/keyboard_cli.py:45
msgid "<keyboardtype> options are:"
msgstr "<keyboardtype> izinketho zingu:"

#: ../src/keyboard_cli.py:56
msgid "You must specify a valid keyboard type."
msgstr "Kumele ucacise uhlobo lwebhodi lezikhiye ezisebenzayo."

#: ../src/keyboard_cli.py:57
msgid "Run 'system-config-keyboard --help' to see a list of keymaps."
msgstr ""
"Sebenzisa ibhodi lakhiye-lokuhlanganisela isistimut - losizo' ukubona uhlu "
"lwamabalazwe kakhiye."

#: ../src/keyboard_cli.py:72
#, python-format
msgid ""
"'%s' is an invalid keymap.  Please run 'system-config-keyboard --help' for a "
"list of keymaps"
msgstr ""
"'%s' liyibalazwe lakhiye elingasebenzi.  Siza usebenzise 'ibhodi lakhiye "
"lenhlanganiselo- usizo' ukuthola uhlu lwebalazwe lakhiye"

#: ../src/keyboard_gui.py:141 ../src/keyboard_gui.py.orig:141
#: ../src/keyboard_tui.py:52
