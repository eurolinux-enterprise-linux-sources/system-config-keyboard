#: ../src/keyboard_gui.py.orig:105
msgid "Keyboard"
msgstr "Bàn phím"

#: ../src/keyboard_cli.py:35
#, fuzzy
msgid ""
"Usage: system-config-keyboard [--help] [--noui] [--text] [<keyboardtype>]\n"
"       --help            Print out this message.\n"
"       --noui            Run in command line mode.\n"
"       --text            Run in text interface mode.\n"
"       "
msgstr ""
"Cách dùng: system-config-keyboard [--help] [--noui] [--text] [<mousetype>]\n"
"       --help            In thông báo này.\n"
"       --noui            Chạy trong chế độ dòng lệnh.\n"
"       --text            Chạy trong chế độ văn bản.\n"
"               \n"
"       <mousetype> là một trong những loại sau: "

#: ../src/keyboard_cli.py:45
msgid "<keyboardtype> options are:"
msgstr "Tùy chọn <keyboardtype> là:"

#: ../src/keyboard_cli.py:56
msgid "You must specify a valid keyboard type."
msgstr "Bạn phải xác định kiểu bàn phím hợp lệ."

#: ../src/keyboard_cli.py:57
#, fuzzy
msgid "Run 'system-config-keyboard --help' to see a list of keymaps."
msgstr "Chạy 'system-config-keyboard --help' để biết danh sách keymap."

#: ../src/keyboard_cli.py:72
#, fuzzy, python-format
msgid ""
"'%s' is an invalid keymap.  Please run 'system-config-keyboard --help' for a "
"list of keymaps"
msgstr ""
"'%s' không phải là keymap hợp lệ. Vui lòng chạy 'system-config-keyboard --"
"help' để biết danh sách keymap."

